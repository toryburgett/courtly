<div class="contentTitle">
  <h1>Cases - <%= link_to "(+)", new_case_path() %></h1>
</div>
<div class="caseAll">
  <% @cases.each do |cse| %>
    <div class="caseSub">
      <div class="caseName">
        <h2><%= link_to "#{cse.plaintiff}   v.   #{cse.defendant}", case_path(cse) %></h2>
      </div>
    </div>
  <% end %>
</div>
